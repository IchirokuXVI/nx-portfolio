{
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "es5",
  "arrowParens": "always",
  "plugins": [
    "prettier-plugin-organize-attributes",
    "prettier-plugin-organize-imports"
  ],
  "attributeGroups": [
    "$ANGULAR_OUTPUT",
    "$ANGULAR_TWO_WAY_BINDING",
    "$ANGULAR_INPUT",
    "$ANGULAR_STRUCTURAL_DIRECTIVE",
    "$DEFAULT",
    "^data-"
  ],
  "attributeSort": "ASC"
}
