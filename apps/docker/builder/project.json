{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "docker/builder",
  "projectType": "application",
  "sourceRoot": "apps/docker/certbot",
  "targets": {
    "build": {
      "executor": "@portfolio/docker:build",
      "options": {
        "imageName": "builder",
        "context": "root"
      },
      "configurations": {
        "development": {
          "noCache": true,
          "tag": "dev"
        },
        "production": {
          "tag": "latest",
          "pushToRegistry": true
        }
      }
    }
  },
  "tags": []
}
